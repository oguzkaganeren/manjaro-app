{
    "expo": {
        "name": "manjaro-news",
        "slug": "manjaro-news",
        "version": "1.0.9",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#1DE9B6"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.manjaro.news",
            "versionCode": 16,
            "permissions": []
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}